{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: archivo actual",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:5500/main.html",
      "webRoot": "${workspaceFolder}/www"
    },
    {
      "name": "Launch Firefox",
      "request": "launch",
      "type": "firefox",
      "reAttach": true,
      "url": "http://127.0.0.1:8080/main.html",
      "webRoot": "${workspaceFolder}/www"
    },
    {
      "name": "Launch Chrome m2",
      "request": "launch",
      "type": "chrome",
      "url": "http://127.0.0.1:8080/main_2.html",
      "webRoot": "${workspaceFolder}/www",
      "skipFiles": [
        "${workspaceFolder}/www/w2/videojs/video.min.js",
        "${workspaceFolder}/www/js/lib/*",
        "${workspaceFolder}/www/js/video.min.js"
      ]
    },
    {
      "name": "Test jwp",
      "request": "launch",
      "type": "chrome",
      "url": "http://127.0.0.1:8080/w2/videojwp.html",
      "webRoot": "${workspaceFolder}/www"
    },
    {
      "name": "Test JS Chrome",
      "request": "launch",
      "type": "chrome",
      "url": "http://127.0.0.1:5500/test/testjs.html",
      "webRoot": "${workspaceFolder}/www",
      "skipFiles": [
        "${workspaceFolder}/www/w2/videojs/video.min.js",
        "${workspaceFolder}/www/js/video.min.js"
      ]
    },
    {
      "name": "Launch Chrome NA",
      "request": "launch",
      "type": "chrome",
      "url": "http://127.0.0.1:8080/main_na.html",
      "webRoot": "${workspaceFolder}/www"
    },
    {
      "name": "Python: server",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/SingleThreadHttpServer.py",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Python: server2",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/HttpServer.py",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/cport/httpServer.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "C:\\msys64\\mingw64\\bin\\gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    }
  ]
}